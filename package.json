{
    "name": "jona.frame",
    "version": "0.3.0",
    "description": "This is the framework used in jona.frl",
    "main": "dist/app.js",
    "scripts": {
        "start": "cross-env MODE='production' node dist/index",
        "build": "rimraf dist && tsc",
        "watch": "cross-env MODE='development' nodemon --ext ts --watch src --exec 'ts-node src'",
        "seed": "ts-node src/seeders",
        "lint": "eslint . --ext .ts",
        "test": "cross-env PORT=9999 DB_NAME=ex_test_db mocha -r ts-node/register tests/**/*.test.ts --no-console"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@types/node-cron": "^2.0.3",
        "bcrypt": "^5.0.0",
        "colors": "^1.4.0",
        "cross-env": "^7.0.2",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "mongoose": "^5.9.20",
        "mongoose-unique-validator": "^2.0.3",
        "node-cron": "^2.0.3"
    },
    "devDependencies": {
        "@types/bcrypt": "^3.0.0",
        "@types/chai": "^4.2.11",
        "@types/express": "^4.17.6",
        "@types/mocha": "^7.0.2",
        "@types/mongoose": "^5.7.28",
        "@types/mongoose-unique-validator": "^1.0.4",
        "@types/node": "^12.12.47",
        "@types/webpack": "^4.41.18",
